# Create connection and cursor
conn = sqlite3.connect('pets.db')
cursor = conn.cursor()

# Insert data into person table
for person_data in data[:4]:
    cursor.execute("INSERT INTO person VALUES (?, ?, ?, ?)", person_data)


# Insert data into pet table
for pet_data in data[4:]:
    cursor.execute("INSERT INTO pet VALUES (?, ?, ?, ?, ?)", pet_data)

# Insert data into person_pet table
person_pet_data = [
    (1, 1),
    (1, 2),
    (2, 3),
    (2, 4),
    (3, 5),
    (4, 6)
]

for person_pet in person_pet_data:
    cursor.execute("INSERT INTO person_pet VALUES (?, ?)", person_pet)

# Commit changes and close connection
conn.commit()
conn.close()
if __name__ == "__main__":
    pass
